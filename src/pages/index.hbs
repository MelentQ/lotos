{{#extend "layout"}}
{{#content "main"}}

<div class="car_video" data-src1="img/auto/1.gif" data-src2="img/auto/2.gif">

  <svg height="100%" width="100%">
    <foreignObject x="0" y="0" width="100%" height="100%">
      <video class="car_video__second_image" xmlns="http://www.w3.org/1999/xhtml" width="1680" height="1050" preload="auto" no-controls autoplay loop playsinline muted>
        <source src="img/auto/2.m4v" type="video/mp4" />
      </video>
    </foreignObject>
    <defs>
      <mask id="cursorMask" x="0" y="0" width="100%" height="100%">
        <g>
          <rect x="0" y="0" width="100%" height="100%"/>
          <circle cx="842" cy="550" r="50" fill="#000000" />
        </g>
      </mask>
    </defs>
    <g>
      <foreignObject id="video-1" x="0" y="0" width="100%" height="100%">
        <video class="bg-car-img" id="video-1" xmlns="http://www.w3.org/1999/xhtml" width="1680" height="1050" preload="auto" no-controls autoplay loop playsinline muted>
          <source src="img/auto/1.m4v" type="video/mp4" />
        </video>
      </foreignObject>
    </g>
  </svg>

