<!-- Банер -->
<div class="bannerContainer js-first-block">
  <div class="container2">
    <div class="banner__content">
      <h1 class="banner__title">ТОРГОВЫЙ ДОМ ЛОТОС</h1>
      <p class="banner__desc">Производственно-коммерческая <br> компания</p>
    </div>
  </div>
  <div class="bannerContainer__social">
    <div class="container">
      <div class="socials">
        <a href="#" target="_blank" class="socials__item icon-fb"></a>
        <a href="#" target="_blank" class="socials__item icon-inst"></a>
        <a href="#" target="_blank" class="socials__item icon-wa"></a>
      </div>
    </div>
  </div>
  <div class="bannerContainer__next">
    <div class="container2">
      <a href="#production" class="go_next">
        Продукция
        <div class="go_next__icon_wrap">
          <div class="go_next__icon">
            <i class="icon-arrow-down"></i>
            <i class="icon-arrow-down"></i>
          </div>
        </div>
      </a>
    </div>
  </div>
</div>
<!-- Банер END -->

<!-- Продукция -->
<div class="production_section" id="production">
  <div class="container2">
    <div class="production__inner">
      <div class="production_section__head">
        <div class="section_title">Продукция</div>
      </div>
      <ul class="production__list">
        <li class="production__item">
          <a href="#" class="production__item-link">
            <div class="production__item-img">
              <img src="img/product.png" alt="">
            </div>
            <p class="production__item-name">Напыляемая шумоизоляция
              и антикоррозионные материалы</p>
          </a>
        </li>
        <li class="production__item">
          <a href="#" class="production__item-link">
            <div class="production__item-img">
              <img src="img/product-3.png" alt="">
            </div>
            <p class="production__item-name">Автомобильные <br>
              ковры</p>
          </a>
        </li>
        <li class="production__item">
          <a href="#" class="production__item-link">
            <div class="production__item-img">
              <img src="img/product-2.png" alt="">
            </div>
            <p class="production__item-name">Керамика</p>
          </a>
        </li>
        <li class="production__item">
          <a href="#" class="production__item-link">
            <div class="production__item-img">
              <img src="img/product-4.png" alt="">
            </div>
            <p class="production__item-name">Защита картера
              и&nbsp;защитная сетка в бампер</p>
          </a>
        </li>
      </ul>
    </div>
  </div>
</div>
<!-- Продукция END -->

<div data-hide-car></div>

<!-- Партнеры -->
<div class="partners_section">
  <div class="container2">
      <h2 class="partners_section__head">Нашими партнерами являются дистрибьюторы и крупные автосалоны по всей России</h2>
  </div>
</div>

{{>partners data=partners title="Наши партнеры"}}
<!-- Партнеры END -->

<!-- География -->
<div class="geography_section">
  <div class="container2">
    <div class="geography_section__title section_title">География продаж</div>
  </div>
  <div data-geography-scale>
    <div class="geography_section__map">
      <div class="container">
        <img src="img/geography.svg" alt="" class="geography_section__map_img">
        <img src="img/geography_mob.svg" alt="" class="geography_section__map_img_mob">
      </div>
    </div>
  </div>
</div>
<!-- География END -->

<!-- Связаться -->
<div class="feedback_section">
  <div class="container2">
    <div class="text_center">
      <button data-popup="contact" type="button" class="feedback_button">
        <span class="feedback_button__text">— Связаться — Связаться — Связаться —</span>
      </button>
    </div>
  </div>
</div>
<!-- Связаться END -->

{{/content}}
{{/extend}}